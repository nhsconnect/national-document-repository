{
    "name": "app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "build-env-check": "node ./react-build-env-checker.js && react-scripts build",
        "test": "vitest - watch",
        "test-all": "vitest",
        "test-all:coverage": "./node_modules/.bin/react-scripts test --watchAll=false --coverage src",
        "format": "prettier --write \"**/*.+(ts|tsx|js|scss|json|css|md)\"",
        "prepare": "cd ../ && husky install ./app/.husky",
        "cypress": "cypress open",
        "cypress-run": "cypress run --env grepTags=regression --browser chrome",
        "cypress-report-run": "cypress run --reporter mochawesome --env grepTags=regression",
        "cypress-report-merge": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesomemerged.json",
        "cypress-report-generate": "npx marge mochawesomemerged.json",
        "cypress-report": "rm -rf cypress/results/* && npm run cypress-report-run && npm run cypress-report-merge && npm run cypress-report-generate"
    },
    "dependencies": {
        "@cypress/grep": "^4.0.1",
        "@testing-library/dom": "10.4.0",
        "@testing-library/jest-dom": "5.17.0",
        "@testing-library/react": "13.4.0",
        "@testing-library/user-event": "13.5.0",
        "@vitejs/plugin-react-swc": "^3.9.0",
        "aws-rum-web": "^1.19.0",
        "aws-sdk": "^2.1510.0",
        "axios": "^1.8.2",
        "dotenv": "^16.3.1",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-legacy-decorators": "^1.0.0",
        "fake-progress": "^1.0.4",
        "govuk-frontend": "^4.9.0",
        "history": "^5.3.0",
        "jwt-decode": "^4.0.0",
        "moment": "^2.30.1",
        "nhsuk-frontend": "^9.3.0",
        "nhsuk-react-components": "^5.0.0",
        "pdfobject": "^2.2.12",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.45.4",
        "react-router": "^6.14.2",
        "react-router-dom": "^6.14.2",
        "react-toggle": "^4.1.3",
        "sass": "^1.66.1",
        "serve": "^14.2.1",
        "typescript": "^4.9.5",
        "usehooks-ts": "^2.9.1",
        "util": "^0.12.5",
        "uuid": "^9.0.1",
        "validator": "^13.11.0",
        "vite": "^6.3.5",
        "vite-plugin-commonjs": "^0.10.4",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-svgr": "^4.3.0",
        "vite-tsconfig-paths": "^5.1.4",
        "web-vitals": "^2.1.4"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@types/jest": "^27.5.2",
        "@types/jest-axe": "^3.5.9",
        "@types/node": "^16.18.40",
        "@types/react": "^18.2.19",
        "@types/react-dom": "^18.2.7",
        "@types/react-toggle": "^4.0.5",
        "@types/uuid": "^9.0.8",
        "@types/validator": "^13.11.8",
        "@vitest/ui": "^3.1.3",
        "babel-plugin-named-exports-order": "^0.0.2",
        "cypress": "^14.2.1",
        "cypress-real-events": "^1.14.0",
        "eslint": "^8.48.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-cypress": "^2.14.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "husky": "^8.0.3",
        "jest-axe": "^8.0.0",
        "jsdom": "^26.1.0",
        "lint-staged": "^15.3.0",
        "mochawesome": "^7.1.3",
        "mochawesome-merge": "^4.3.0",
        "prettier": "^3.0.3",
        "prop-types": "^15.8.1",
        "vitest": "^3.1.3",
        "vitest-axe": "^0.1.0",
        "webpack": "^5.97.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "moduleNameMapper": {
            "axios": "axios/dist/node/axios.cjs"
        }
    }
}
